{
  "extends": [
    "plugin:storybook/recommended",
    "next/core-web-vitals",
    "eslint:recommended",
    "next",
    "prettier"
  ],
  "rules": {
    "no-unused-vars": "warn"
  },
  "overrides": [
    {
      "files": ["*.stories.@(ts|tsx|js|jsx|mjs|cjs)"],
      "rules": {
        "storybook/hierarchy-separator": "error",
      }
    }
  ],
  "env": {
    "jest": true
  }
}
